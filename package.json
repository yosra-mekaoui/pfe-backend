{
  "name": "my_backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "start": "node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/server.js",
    "debug": "npm run dev -- --inspect",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./src",
    "pre-test": "node ./test/index.js",
    "test": "cross-env NODE_ENV=test npm run pre-test && nyc mocha ./test/**/*.test.js",
    "test:ot": "cross-env NODE_ENV=test nyc mocha ./test/**/*.test.js",
    "test1": "mocha './test/**/*.test.js' --reporter mochawesome --require mochawesome/register",
    "test:coverage": "nyc --reporter=html mocha ./test/**/*.test.js",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Yosra Mekaoui",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.5",
    "babel-jest": "^28.1.3",
    "chai": "^4.4.1",
    "chai-http": "^4.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-report-generator": "^6.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "swagger-autogen": "^2.23.7",
    "util": "^0.12.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.13.0"
  }
}
